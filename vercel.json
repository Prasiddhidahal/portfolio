{
  "builds": [
    {
      "src": "personal_portfolio/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
      "src": "manage.py",
      "use": "@vercel/static-build",
      "config": { "distDir": "static" }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "personal_portfolio/wsgi.py"
    }
  ]
}
